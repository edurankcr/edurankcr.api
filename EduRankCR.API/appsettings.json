{
  "ConnectionStrings": {
    "DefaultConnection": "<CONNECTION_STRING>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "Jwt": {
    "Key": "<KEY>",
    "Issuer": "<ISSUER>",
    "Audience": "<AUDIENCE>",
    "ExpiresInMinutes": 60
  },
  "AllowedHosts": "*"
}
